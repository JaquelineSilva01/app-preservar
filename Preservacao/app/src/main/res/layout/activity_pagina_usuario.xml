<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TelaUsuario">

    <Button
        android:id="@+id/bt_dsm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:background="#4CAF50"
        android:text="Desmatamento"
        app:backgroundTint="#4CAF50"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/bt_qmd"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="#4CAF50"
        android:text="Queimdas"
        app:backgroundTint="#4CAF50"
        app:layout_constraintEnd_toEndOf="@+id/bt_dsm"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bt_dsm"
        app:layout_constraintTop_toBottomOf="@+id/bt_dsm" />

    <Button
        android:id="@+id/bt_psv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="#4CAF50"
        android:text="API"
        app:backgroundTint="#4CAF50"
        app:layout_constraintEnd_toEndOf="@+id/bt_qmd"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bt_qmd"
        app:layout_constraintTop_toBottomOf="@+id/bt_qmd" />

    <Button
        android:id="@+id/bt_pf"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="#4CAF50"
        android:text="Perfil"
        app:backgroundTint="#4CAF50"
        app:layout_constraintEnd_toEndOf="@+id/bt_psv"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bt_psv"
        app:layout_constraintTop_toBottomOf="@+id/bt_psv" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="202dp"
        android:layout_height="164dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/preserve" />

    <Button
        android:id="@+id/sair_app"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="#4CAF50"
        android:text="Sair"
        app:backgroundTint="#4CAF50"
        app:layout_constraintEnd_toEndOf="@+id/bt_pf"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="@+id/bt_pf"
        app:layout_constraintTop_toBottomOf="@+id/bt_pf" />
</androidx.constraintlayout.widget.ConstraintLayout>